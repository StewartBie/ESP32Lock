/**************************/
/***************指纹识别************/
void Press_FR()
{
  char ensure, i;
  char str[20];
  int k;
  k = digitalRead(15);
  if (k == 1)
  { //指纹触摸是否输出高电平？
    oled_door_finger_press();
    digitalWrite(4, LOW); //是，则把4继电器输出低电平，点亮指纹识别
    delay(300);
    for (i = 0; i < 5; i++)
    {
      ensure = finger.getImage();
      if (ensure == 0x00)         //获取图像成功
      {
        ensure = finger.image2Tz();
        if (ensure == 0x00)       //生成特征成功
        {
          ensure = finger.fingerFastSearch();
          if (ensure == 0x00)    //搜索成功
          {
            Serial.print("finger OK");
            oled_door_open();
            beer_one();
            digitalWrite(4, HIGH);//关闭指纹识别
            door();                      //开门
            break;
          }
          else
          {
            Serial.print("finger ERROR");//搜索失败
            oled_door_finger_err();
            beer_two();         //蜂鸣器响两声，识别错误
            break;
          }
        }
        else
        { //获取图像失败
        }
      }
      delay(100);
      if (i == 4) {
        oled_door_finger_miss();
        beer_three();//最后一次循环都没有检测到任何指纹，无论是否匹配，蜂鸣器向三声，获取图像失败
      }
    }
    delay(500);
    digitalWrite(4, HIGH);//关闭指纹识别
    delay(500);
  }
}
