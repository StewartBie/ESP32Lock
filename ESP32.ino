/***********电池电量测量，充电状态，门状态，门外是否有人状态**********/
void bat() {
  Serial.print("电池电压是:  ");
  Serial.print(analogRead(ADC1));
  Serial.print("  ");
  bat_voltage = ((analogRead(ADC1) * 4.2) / 40000); //(当前adc*满电电压4.2)/满电adc
  Serial.print(bat_voltage);
  Serial.println(" V");
  Serial.print("电池电量估计值:  ");
  bat_value = (((analogRead(ADC1) - 31500) * 100) / 8500); //((当前adc-没电adc)*100)/满电减没电的adc差
  if (bat_value > 100) {
    bat_value = 100;
  }
  else if (bat_value < 0) {
    bat_value = 0;
  }
  Serial.print(bat_value);
  Serial.println(" %");
  Serial.print("  ");
  return;
}

void state() {
  int i, j, k;
  bool result = Blinker.connect();//获取联网状态
  int count = ((millis()) % 10000);//数字从0到9999变化，时间为10s
  int timeflag = ((millis()) / 10000) % 2;//每隔10s timeflag在0、1之间变化

  if (timeflag == oledflag) { //两个flag不等时会执行1次，不等的时间是timeflag变化一次的时间
    bat();//电池测量一次电压
    if (oledflag == 0)
      oledflag = 1;
    else
      oledflag = 0;

    Serial.println("系统运行时长(ms)");
    Serial.println(millis());//打印一下运行时长，秒xxx
    Serial.println();
  }



  k = digitalRead(13); //充电检测
  if (k == 1)
    bat_charge = 1; //1充电中
  else
    bat_charge = 0; //0没在充电

  j = digitalRead(19); //门霍尔传感器检测
  if (j == 1)
    doorflag = 1; //1门开着
  else
    doorflag = 0; //0门关着


  if (result == 0 && ((count <= 2500) || (count > 5000 && count <= 7500)))
    digitalWrite(27, HIGH);
  else
    digitalWrite(27, LOW);

  if (((WiFi.status() != WL_CONNECTED) == 1) && ((count <= 2500) || (count > 5000 && count <= 7500)))
    digitalWrite(26, HIGH);
  else
    digitalWrite(26, LOW);

  if (bat_value <= 20 && ((count <= 2500) || (count > 5000 && count <= 7500)))
    digitalWrite(25, HIGH);
  else
    digitalWrite(25, LOW);


  if (((WiFi.status() != WL_CONNECTED) || (result == 0)) && (millis() > 1800000)) //如果wifi没连接，或者断网，并且超过1800秒，即半小时
  {
    u8g2.clearBuffer();//清屏
    u8g2.setFont(u8g2_font_ncenB14_tr);//14和08
    u8g2.setCursor(20, 30);//设置显示位置
    u8g2.print("Reboot!");//
    u8g2.sendBuffer();
    delay(5000);
    esp_restart();//软重启
  }

}
